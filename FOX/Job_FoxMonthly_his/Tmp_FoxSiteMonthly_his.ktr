    <sql>select &#xa;	date_format&#x28;DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d;&#x2b;1 month&#x29;,&#x27;&#x25;Y-&#x25;m&#x27;&#x29; as YM,&#xa;	date_format&#x28;DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d;&#x2b;1 month&#x29;,&#x27;&#x25;M-&#x25;Y&#x27;&#x29; as DisplayYM,&#xa;	c.website_type as WebsiteType,&#xa;    b.display_name as WebsiteName,&#xa;	case when a.clip_duration &#x3e;&#x3d; 600 then 1 else 0 end as DurationFlag,&#xa;	d.country_name as HostCountry,&#xa;	count&#x28;&#x2a;&#x29; as Reported,&#xa;    sum&#x28;case&#xa;        when a.takeoff_time &#x3e; 0 and a.takeoff_time &#x3c; DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d; month&#x29;&#xa;		and a.count_send_notice &#x3e;0 then 1&#xa;        else 0&#xa;    end&#x29; as RemovedTotal,&#xa;	sum&#x28;case&#xa;        when a.takeoff_time &#x3e; 0 and a.takeoff_time &#x3c; DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d; month&#x29;&#xa;		and a.count_send_notice &#x3d;1 then 1&#xa;        else 0&#xa;    end&#x29; as RemovedOneNotice,&#xa;	sum&#x28;case&#xa;        when a.takeoff_time &#x3e; 0 and a.takeoff_time &#x3c; DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d; month&#x29;&#xa;		and a.count_send_notice &#x3e;1 then 1&#xa;        else 0&#xa;    end&#x29; as RemovedLargeOneNotice,&#xa;    sum&#x28;v.views&#x29; as Views,&#xa;	current_timestamp as ETL_DTE&#xa;    &#xa;from&#xa;    tracker2.matchedVideo a,&#xa;    tracker2.trackingWebsiteExtraInfo b,&#xa;	tracker2.metaExtraInfo me,&#xa;    mddb.trackingWebsite c&#xa;        left join&#xa;    mddb.country d ON c.country_id &#x3d; d.id,&#xa;    &#x28;select &#xa;        c.id,&#xa;            SUM&#x28;CASE&#xa;                WHEN c.max_value &#x3e; IFNULL&#x28;c.min_value, 0&#x29; THEN c.max_value - IFNULL&#x28;c.min_value, 0&#x29;&#xa;                ELSE 0&#xa;            END&#x29; views&#xa;    from&#xa;        &#x28;SELECT &#xa;			a.id,&#xa;            MAX&#x28;CASE&#xa;                WHEN b.created_at &#x3c; DATE_SUB&#x28;DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d;&#x2b;1 month&#x29;, INTERVAL 7 hour&#x29; THEN b.view_count&#xa;                ELSE null&#xa;            END&#x29; min_value,&#xa;            MAX&#x28;CASE&#xa;                WHEN&#xa;                    b.created_at &#x3e;&#x3d; date_sub&#x28;DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d;&#x2b;1 month&#x29;, INTERVAL 7 hour&#x29;&#xa;                        AND b.created_at &#x3c; date_sub&#x28;DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d; month&#x29;, INTERVAL 7 hour&#x29;&#xa;                THEN&#xa;                    b.view_count&#xa;                ELSE 0&#xa;            END&#x29; max_value&#xa;		FROM&#xa;			tracker2.matchedVideo a&#xa;			left join tracker2.matchedVideoViewCountHis b ON a.id &#x3d; b.matchedVideo_id&#xa;			and b.company_id &#x3d; 10&#xa;		where&#xa;			a.company_id &#x3d; 10&#xa;				and a.first_send_notice_date &#x3e;&#x3d; DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d;&#x2b;1 month&#x29;&#xa;				AND a.first_send_notice_date &#x3c; DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d; month&#x29;&#xa;				and a.count_send_notice &#x3e; 0&#xa;				and a.hide_flag &#x3d; 2&#xa;		GROUP BY a.id&#x29; c&#xa;    group by 1&#x29; v&#xa;where&#xa;    a.trackingWebsite_id &#x3d; c.id&#xa;        and a.trackingWebsite_id &#x3d; b.trackingWebsite_id&#xa;		and a.trackingMeta_id &#x3d; me.meta_id&#xa;        and a.company_id &#x3d; 10&#xa;        and a.first_send_notice_date &#x3e;&#x3d; DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d;&#x2b;1 month&#x29;&#xa;        and a.first_send_notice_date &#x3c; DATE_SUB&#x28;&#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;, INTERVAL &#x24;&#x7b;STEP&#x7d; month&#x29;&#xa;        and a.count_send_notice &#x3e; 0&#xa;        and hide_flag &#x3d; 2&#xa;        and c.website_type in &#x28;&#x27;ugc&#x27; , &#x27;hybrid&#x27;&#x29;&#xa;        and a.id &#x3d; v.id&#xa;		and &#x28;me.trackingSetting_id &#x3e; 0 or &#xa;			&#x28;me.trackingSetting_id &#x3d; 0 and me.stop_at &#x3e;&#x3d; &#x27;2016-01-01 07&#x3a;00&#x3a;00&#x27;&#x29;&#x29;&#xa; group by c.website_type, b.display_name, case when a.clip_duration &#x3e;&#x3d; 600 then 1 else 0 end,&#xa;			d.country_name&#x3b;</sql>
