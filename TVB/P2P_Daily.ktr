    <sql>select distinct&#xa;    date&#x28;a.created_at&#x29; as ReportDate,&#xa;    a.meta_title as VideoTitle,&#xa;    e.link_url as WebsiteURL,&#xa;    a.clip_url as SourceURL,&#xa;    a.clip_title as FileName,&#xa;    a.view_count as Infringers,&#xa;    case&#xa;        when&#xa;            a.matched_video_duration &#x3e; 0&#xa;                and a.matched_audio_duration &#x3e; 0&#xa;        then&#xa;            &#x27;Audio&#x2f;Video&#x27;&#xa;        when a.matched_video_duration &#x3e; 0 then &#x27;Video&#x27;&#xa;        when a.matched_audio_duration &#x3e; 0 then &#x27;Audio&#x27;&#xa;        else &#x27;N&#x2f;A&#x27;&#xa;    end as MatchType,&#xa;    a.clip_duration as ClipDuration,&#xa;    a.matched_video_duration&#x2a;100&#x2f;a.clip_duration as VPercent,&#xa;    a.matched_audio_duration&#x2a;100&#x2f;a.clip_duration as APercent,&#xa;    case&#xa;        when a.takeoff_time &#x3e; 0 then &#x27;Removed&#x27;&#xa;        when&#xa;            a.takeoff_time &#x3d; 0&#xa;                and a.count_send_notice &#x3d; 0&#xa;                and a.is_in_takedown_queue &#x3d; &#x27;false&#x27;&#xa;        then&#xa;            &#x27;Active&#x27;&#xa;        when&#xa;            a.takeoff_time &#x3d; 0&#xa;                and a.count_send_notice &#x3e; 0&#xa;        then&#xa;            &#x27;sent-Not Removed&#x27;&#xa;        when a.is_in_takedown_queue &#x3d; &#x27;true&#x27; then &#x27;in_queue&#x27;&#xa;        when is_contentRule_applied &#x3d; &#x27;true&#x27; then &#x27;Filtered&#x27;&#xa;        else &#x27;&#x27;&#xa;    end as Status,&#xa;    a.count_send_notice as TakeDownSentNO,&#xa;    nullif&#x28;a.last_send_notice_date,0&#x29; as LastSentDateTime,&#xa;    tx.display_name as Protocol,&#xa;    &#x28;select &#xa;            group_concat&#x28;distinct country_name&#xa;                    SEPARATOR &#x27;&#x3b;&#x27;&#x29;&#xa;        from&#xa;            mddb.country co,&#xa;            matchedVideoP2PItem mi&#xa;        where&#xa;            co.id &#x3d; mi.country_id&#xa;                and mi.matchedVideo_id &#x3d; a.id&#x29; as ISPCountry,&#xa;    f.keyname as Classification,&#xa;	current_timestamp as ETL_DTE&#xa;from&#xa;    matchedVideo a&#xa;        left join&#xa;    matchedVideo_linkURL e ON a.id &#x3d; e.matchedVideo_id&#xa;        and e.company_id &#x3d; 1333,&#xa;    mddb.trackingWebsite b,&#xa;    mddb.meta c&#xa;        left join&#xa;    mddb.TVContent d ON c.id &#x3d; d.meta_id,&#xa;    company_classification f,&#xa;    tracking_list tl,&#xa;    tracking_listitem tli,&#xa;    trackingWebsiteExtraInfo tx&#xa;where&#xa;    a.trackingWebsite_id &#x3d; b.id&#xa;        and a.trackingMeta_id &#x3d; c.id&#xa;        and a.company_id &#x3d; 1333&#xa;        and b.website_type &#x3d; &#x27;p2p&#x27;&#xa;        and a.hide_flag &#x3d; f.keyvalue&#xa;        and a.trackingMeta_id &#x3d; tli.tracking_meta_id&#xa;        and tli.tracking_list_id &#x3d; tl.id&#xa;        and tl.company_id &#x3d; 1333&#xa;        and tl.name &#x3d; &#x27;TVB TV List&#x27;&#xa;        and b.id &#x3d; tx.trackingWebsite_id&#xa;        and a.is_poster_whitelist_filtered &#x3d; &#x27;false&#x27;&#x3b;</sql>
    <limit>0</limit>
    <lookup/>
