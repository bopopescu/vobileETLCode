    <connection>tracker2</connection>
    <sql>&#xa;SELECT  YM,&#xa;	Websitetype,&#xa;    websiteName,&#xa;	ifnull&#x28;b.country_name, &#x27;unknown&#x27;&#x29; as &#x27;HostCountry&#x27;,&#xa;	trackingWebsite_id,&#xa;    TakedownNum,&#xa;	Sum_Takeoff_minutes_duration,&#xa;	current_timestamp ETL_DTE&#xa;FROM &#xa;&#x28;SELECT  &#xa;	DATE_FORMAT&#x28;date_add&#x28;a.first_send_notice_date, interval -8 hour&#x29;, &#x27;&#x25;Y-&#x25;m&#x27;&#x29; &#x27;YM&#x27;,&#xa;	b.website_type &#x27;Websitetype&#x27;,&#xa;    b.display_name &#x27;websiteName&#x27;,&#xa;	b.country_id,&#xa;	a.trackingWebsite_id,&#xa;    SUM&#x28;if&#x28;a.takeoff_time &#x3e; 0, 1, 0&#x29;&#x29; &#x27;TakedownNum&#x27;,&#xa;	sum&#x28;case when a.first_send_notice_date &#x3e;0 and a.takeoff_time&#x3e;0  then TIMESTAMPDIFF&#x28;MINUTE, a.first_send_notice_date, a.takeoff_time&#x29; else 0  end&#x29; as &#x27;Sum_Takeoff_minutes_duration&#x27;&#xa;FROM tracker2.matchedVideo a,&#xa;     mddb.trackingWebsite b&#xa;WHERE a.trackingWebsite_id &#x3d; b.id&#xa;  AND a.count_send_notice&#x3e;0&#xa;  AND a.hide_flag &#x3d; 2&#xa;  AND a.company_id &#x3d; 14&#xa;  AND b.website_type in &#x28;&#x27;ugc&#x27;, &#x27;hybrid&#x27;&#x29;&#xa;  AND date_format&#x28;a.first_send_notice_date, &#x27;&#x25;Y-&#x25;m-&#x25;d &#x25;T&#x27;&#x29; &#x3e;&#x3d; &#x27;&#x24;&#x7b;STARTDATE&#x7d;&#x27;&#xa;  AND date_format&#x28;a.first_send_notice_date, &#x27;&#x25;Y-&#x25;m-&#x25;d &#x25;T&#x27;&#x29; &#x3c; &#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;&#xa;GROUP BY 1, 2, 3, 4, 5&#x29; a&#xa;LEFT JOIN mddb.country b ON b.id &#x3d; a.country_id&#xa;&#xa;UNION ALL&#xa;&#xa;SELECT  YM,&#xa;	Websitetype,&#xa;    websiteName,&#xa;	ifnull&#x28;b.country_name, &#x27;unknown&#x27;&#x29; as &#x27;HostCountry&#x27;,&#xa;	trackingWebsite_id,&#xa;    TakedownNum,&#xa;	Sum_Takeoff_minutes_duration,&#xa;	current_timestamp ETL_DTE&#xa;FROM &#xa;&#x28;SELECT  &#xa;	DATE_FORMAT&#x28;date_add&#x28;a.first_send_notice_date, interval -8 hour&#x29;, &#x27;&#x25;Y-&#x25;m&#x27;&#x29; &#x27;YM&#x27;,&#xa;	b.website_type &#x27;Websitetype&#x27;,&#xa;    b.display_name &#x27;websiteName&#x27;,&#xa;	b.country_id,&#xa;	a.trackingWebsite_id,&#xa;    SUM&#x28;if&#x28;a.takeoff_time &#x3e; 0, 1, 0&#x29;&#x29; &#x27;TakedownNum&#x27;,&#xa;	sum&#x28;case when a.first_send_notice_date &#x3e;0 and a.takeoff_time&#x3e;0  then TIMESTAMPDIFF&#x28;MINUTE, a.first_send_notice_date, a.takeoff_time&#x29; else 0  end&#x29; as &#x27;Sum_Takeoff_minutes_duration&#x27;&#xa;FROM tracker2.matchedVideo a,&#xa;     mddb.trackingWebsite b,&#xa;     tracker2.matchedFileItem d&#xa;WHERE d.matchedFile_id &#x3d;  a.matchedFile_id&#xa;  AND a.trackingWebsite_id &#x3d; b.id&#xa;  AND a.count_send_notice&#x3e;0&#xa;  AND a.hide_flag &#x3d; 2&#xa;  AND a.company_id &#x3d; 14&#xa;  AND b.website_type &#x3d; &#x27;cyberlocker&#x27;&#xa;  AND date_format&#x28;a.first_send_notice_date, &#x27;&#x25;Y-&#x25;m-&#x25;d &#x25;T&#x27;&#x29; &#x3e;&#x3d; &#x27;&#x24;&#x7b;STARTDATE&#x7d;&#x27;&#xa;  AND date_format&#x28;a.first_send_notice_date, &#x27;&#x25;Y-&#x25;m-&#x25;d &#x25;T&#x27;&#x29; &#x3c; &#x27;&#x24;&#x7b;ENDDATE&#x7d;&#x27;&#xa;GROUP BY 1, 2, 3, 4, 5&#x29; a&#xa;LEFT JOIN mddb.country b ON b.id &#x3d; a.country_id&#xa;&#xa;</sql>
